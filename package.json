{
  "name": "flatten-object-deep",
  "version": "0.0.1",
  "description": "flatten your POJO",
  "main": "lib/index.js",
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "cross-env": "^1.0.8",
    "ghooks": "^1.3.2",
    "rimraf": "^2.5.3",
    "standard": "^7.1.2",
    "webpack": "^1.13.1"
  },
  "scripts": {
    "clean": "rimraf dist lib",
    "test": "ava",
    "check": "standard && npm test",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel index.js --out-dir lib",
    "build:umd": "cross-env NODE_ENV=development webpack --display-modules index.js dist/flatten-object-deep.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack --display-modules index.js dist/flatten-object-deep.min.js",
    "build": "npm run clean && npm run build:commonjs && npm run build:umd && npm run build:umd:min",
    "prepublish": "npm run check && npm run build"
  },
  "repository": {
    "type": "git",
    "repo": "https://github.com/danne931/flatten-object-deep.git"
  },
  "author": "Daniel Eisenbarger <danieleisenbarger@gmail.com> (https://github.com/danne931)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danne931/flatten-object-deep/issues"
  },
  "keywords": [
    "flatten",
    "plain old javascript object",
    "pojo"
  ],
  "standard": {
    "parser": "babel-eslint"
  },
  "ava": {
    "files": [
      "test"
    ],
    "failFast": "true",
    "verbose": "true",
    "require": [
      "babel-core/register"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "standard",
      "pre-push": "npm test"
    }
  }
}
